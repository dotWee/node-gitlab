{
  "name": "gitlab",
  "version": "3.10.1",
  "description": "Full NodeJS implementation of the GitLab API. Supports Promises, Async/Await.",
  "main": "dist/latest/index.js",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "prepush": "npm run lint",
    "commit": "npx git-cz",
    "build:clean": "rimraf -rf dist && mkdir -p dist",
    "build:clean-tsc": "rimraf -rf dist-tsc && mkdirp -p dist-tsc",
    "build:es6": "babel src -d dist/latest --config-file=./.babelrc",
    "build:es6-tsc": "tsc -p tsconfig.dist.json",
    "build:es5": "babel src -d dist/es5  --config-file=./.babelrc-es5",
    "build:es5-tsc": "tsc -p tsconfig-es5.dist.json",
    "build": "npm run build:clean && npm run build:es6 && npm run build:es5",
    "build-tsc": "npm run build:clean-tsc && npm run build:es6-tsc && npm run build:es5-tsc",
    "lint": "node_modules/.bin/eslint ./src ./test",
    "lint-tsc": "tslint 'src/**/*.{ts,tsx}' --format stylish",
    "test:bundles": "jest test/tests/bundles",
    "test:infrastructure": "jest test/tests/infrastructure",
    "test:services": "jest test/tests/services",
    "test": "jest && codecov",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdalrymple/node-gitlab"
  },
  "keywords": [
    "gitlab",
    "api",
    "es6",
    "es5",
    "request"
  ],
  "bugs": {
    "url": "https://github.com/jdalrymple/node-gitlab/issues"
  },
  "author": "Justin Dalrymple <justin.s.dalrymple@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@babel/cli": "7.0.0-rc.1",
    "@babel/core": "7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-rc.1",
    "@semantic-release/changelog": "^3.0.0",
    "semantic-release": "^15.9.8",
    "@types/humps": "^1.1.2",
    "@types/qs": "^6.5.1",
    "@types/parse-link-header": "^1.0.0",
    "@types/request-promise": "^4.1.41",
    "@types/url-join": "^0.8.2",
    "@types/util.promisify": "^1.0.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/npm": "^5.0.2",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "codecov": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-extended": "^0.8.1",
    "jest-tap-reporter": "^1.9.0",
    "regenerator-runtime": "^0.12.1"
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-rc.1",
    "humps": "^2.0.1",
    "parse-link-header": "^1.0.1",
    "qs": "^6.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-core": "^1.1.1",
    "url-join": "^4.0.0",
    "util.promisify": "^1.0.0",
    "xhr": "^2.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
